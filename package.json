{
  "name": "gatsby-source-wagtail",
  "description": "Gatsby plugin which adds Wagtail Grapple support to Gatsby",
  "version": "1.0.0",
  "author": "NathHorrigan <NathHorrigan@gmail.com>",
  "bugs": {
    "url": "https://github.com/NathHorrigan/gatsby-source-wagtail/issues"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "main": "index.js",
  "files": [
    "*.js",
    "babel-plugin-remove-graphql-queries/*.js",
    "index.d.ts"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@graphql-tools/delegate": "^8.4.3",
    "@graphql-tools/links": "^8.2.1",
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/wrap": "^8.3.2",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-link-ws": "^1.0.20",
    "apollo-utilities": "^1.3.4",
    "fs-extra": "^8.1.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^8.2.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "react": "^17.0.0",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "babel-preset-gatsby-package": "^2.5.0-next.0",
    "cross-env": "^7.0.3",
    "prettier": "^2.5.1"
  },
  "homepage": "https://github.com/NathHorrigan/gatsby-source-wagtail//#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "wagtail"
  ],
  "peerDependencies": {
    "gatsby": "^4.0.0",
    "gatsby-source-filesystem": "4.3.0",
    "gatsby-source-graphql": "4.3.0"
  },
  "resolutions": {
    "graphql": "14.5.0"
  },
  "license": "MIT",
  "repository": "https://github.com/NathHorrigan/gatsby-source-wagtail/",
  "scripts": {
    "build": "babel src/ --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "format": "prettier --write src/**/*.js",
    "clean": "rm -rf ./*.js gatsby-node gatsby-ssr"
  }
}
